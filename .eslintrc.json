{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:@next/next/recommended"
    ],
    "plugins": ["@typescript-eslint"],
    "globals": {
        "JSX":"readonly"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "ignorePatterns": ["*.config.js"],
    "root": true,
    "rules": {
        // "no-unused-vars": "off",
        // "@typescript-eslint/no-unused-vars": ["error"]
    }
}
